/* pages/match/index.wxss */
.canvas-container {
  display: block;
  position: fixed;
  top: -1000rpx;
  left: -1000rpx;
  visibility: hidden;
}

.upload-container {
  display: flex;
}
.upload {
  height: 500rpx;
  width: 50%;
  background: #eee;
  border: 1px solid #ddd;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  text-align: center;
}
.upload text {
  margin: 0 auto;
  display: block;
  height: 100%;
  width: 100%;
  line-height: 500rpx;
}
.upload image{
  width: 100%;
  height: 100%;
}